<CEAConfiguration>
    <ActiveSyncServerConfigurations>
        <ActiveSyncServerConfiguration>
            <Key>exchange-online</Key>
            <EnforcementService>io.entgra.device.mgt.core.cea.mgt.enforce.Impl.ExchangeOnlineCEAEnforcementServiceImpl</EnforcementService>
            <GatewayService>io.entgra.device.mgt.core.cea.mgt.enforce.Impl.gateway.ExchangeOnlineGatewayServiceImpl</GatewayService>
        </ActiveSyncServerConfiguration>
    </ActiveSyncServerConfigurations>
    <MonitoringConfiguration>
        <MonitoringEnable>true</MonitoringEnable>
        <MonitoringFrequency>180000</MonitoringFrequency>
        <MonitoringClazz>io.entgra.device.mgt.core.cea.mgt.core.task.CEAPolicyMonitoringTask</MonitoringClazz>
    </MonitoringConfiguration>
</CEAConfiguration>